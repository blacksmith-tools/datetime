{
  "name": "@descco/ui",
  "version": "v0.4.4",
  "description": "Descco UI",
  "scripts": {
    "build:react": "cross-env NODE_ENV=production babel-node bin/build/build-cli.js && webpack --progress --config ./internals/webpack/react/webpack.config.js",
    "build:storybook": "cross-env NODE_ENV=development build-storybook -c ./internals/storybook -o .out",
    "lint": "yarn lint:sass && yarn lint:js",
    "lint:js": "eslint source/**",
    "lint:sass": "sass-lint 'source/**/*.scss' --verbose --no-exit",
    "precommit": "yarn lint",
    "prepublish": "yarn build",
    "prepush": "jest --setupTestFrameworkScriptFile=internals/test/setup.js",
    "storybook": "start-storybook -p 9000 -c ./internals/storybook",
    "test": "jest --setupTestFrameworkScriptFile=internals/test/setup.js",
    "test:storybook": "storyshots -c ./internals/storybook",
    "play": "vbuild -c play.config.js -d",
    "build:play": "vbuild -c play.config.js"
  },
  "main": "lib/index.js",
  "keywords": [
    "descco",
    "theme"
  ],
  "author": "Hemerson Vianna <hemerson.lourenco@b2wdigital.com> (https://github.com/hemersonvianna)",
  "peerDependencies": {
    "prop-types": ">=15.5.10",
    "react": ">=15.6.1",
    "react-dom": ">=15.6.1"
  },
  "dependencies": {
    "babel-loader": "7.1.1",
    "classnames": "2.2.5",
    "immutable": "3.8.1",
    "moment": "2.18.1",
    "node-sass": "4.5.3",
    "paginator": "1.0.0",
    "prop-types": "15.5.10",
    "react-addons-css-transition-group": "15.5.2",
    "react-bootstrap-table": "3.1.6",
    "react-css-modules": "4.0.3",
    "react-onclickoutside": "5.9.0",
    "react-redux": "5.0.4",
    "react-select": "1.0.0-rc.2",
    "react-tagsinput": "3.15.1",
    "react-transition-group": "1.1.3",
    "redux": "3.6.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.1",
    "redux-thunk": "2.2.0",
    "svg-react-loader": "0.3.7"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.3.0",
    "@kadira/storybook": "2.21.0",
    "@kadira/storybook-addon-knobs": "1.7.1",
    "@kadira/storybook-addon-options": "1.0.1",
    "@kadira/storybook-addons": "1.6.1",
    "@kadira/storyshots": "2.1.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "7.1.0",
    "babel-jest": "17.0.2",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "child-process-promise": "^2.2.0",
    "colors": "1.1.2",
    "cross-env": "3.1.3",
    "css-loader": "0.26.0",
    "eslint": "3.10.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-loader": "1.4.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "fs-extra": "^2.1.2",
    "fs-promise": "^2.0.2",
    "husky": "0.13.3-0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.7.0",
    "jest-cli": "17.0.2",
    "postcss-loader": "1.1.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-test-renderer": "15.5.4",
    "rimraf": "2.6.1",
    "sass-lint": "1.10.2",
    "sass-loader": "4.1.0",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "vbuild": "6.24.14",
    "vue": "2.2.6",
    "vue-play": "^3.1.3",
    "vue-play-cli": "1.1.1",
    "vuex": "2.2.1",
    "webpack": "3.4.1",
    "webpack-dev-server": "2.6.1",
    "yargs": "7.0.2"
  },
  "engines": {
    "node": ">=6.9.2",
    "npm": ">=3.10.8"
  },
  "jest": {
    "verbose": false,
    "testRegex": "(/shared/.*.(test|spec)).(js|jsx)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(s?css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./internals/file-transformer.js"
    },
    "moduleDirectories": [
      "node_modules",
      "shared"
    ]
  }
}
